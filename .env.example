# Production environment variables
# Copy to .env and fill in the values

# PostgreSQL
POSTGRES_PASSWORD=your-secure-postgres-password

# Identity Service KEKs (generate with: openssl rand -base64 32)
KMS_KEKBASE64=your-32-byte-base64-kek-for-signing-keys
SECURITY_MFA_KEKBASE64=your-32-byte-base64-kek-for-mfa-secrets
