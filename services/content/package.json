    "@opentelemetry/instrumentation-express": "^0.33.0",
    "@opentelemetry/instrumentation-mongoose": "^0.33.0",
    "crypto": "^1.0.1",
    "uuid": "^9.0.0",
    "semver": "^7.5.4",
    "node-cron": "^3.0.2"
