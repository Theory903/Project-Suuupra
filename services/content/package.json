    "@aws-sdk/s3-request-presigner": "^3.400.0",
    "socket.io": "^4.7.2",
    "bullmq": "^4.10.0",
    "ioredis": "^5.3.2",
    "ajv": "^8.12.0",
    "ajv-formats": "^2.1.1",
    "helmet": "^7.0.0",
    "cors": "^2.8.5",
    "compression": "^1.7.4",
    "express-rate-limit": "^6.10.0",
    "express-validator": "^7.0.1",
    "multer": "^1.4.5-lts.1",
    "multer-s3": "^3.0.1",
    "dompurify": "^3.0.5",
    "jsdom": "^22.1.0",
