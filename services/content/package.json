{
  "name": "@suuupra/content-service",
  "version": "1.0.0",
  "description": "Production-grade content management service",
  "main": "dist/server.js",
  "scripts": {
    "dev": "nodemon src/server.ts",
    "build": "tsc",
    "start": "node dist/server.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:integration": "jest --config jest.integration.config.js",
    "test:load": "k6 run tests/load/content-load-test.js",
    "migrate": "node dist/migrations/runner.js",
    "seed": "node dist/scripts/seed.js",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "docker:dev": "docker-compose -f docker-compose.dev.yml up",
    "docker:prod": "docker-compose -f docker-compose.yml up"
  },
  "dependencies": {
    "express": "^4.18.2",
