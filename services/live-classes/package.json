{
  "name": "live-classes-service",
  "version": "1.0.0",
  "description": "Production-ready real-time interactive live classes service with WebRTC and mediasoup",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "tsx watch src/index.ts",
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "test:integration": "jest --config jest.integration.config.js",
    "lint": "eslint src --ext .ts,.js",
    "lint:fix": "eslint src --ext .ts,.js --fix",
    "docker:build": "./scripts/build.sh",
    "docker:run": "docker-compose up -d",
    "k8s:deploy": "./scripts/deploy.sh"
  },
  "keywords": ["webrtc", "live-streaming", "education", "mediasoup"],
  "author": "Suuupra EdTech",
  "license": "PROPRIETARY",
  "dependencies": {
    "fastify": "^4.24.3",
    "@fastify/websocket": "^8.3.1",
    "@fastify/cors": "^8.4.0",
    "@fastify/helmet": "^11.1.1",
    "@fastify/rate-limit": "^8.0.3",
    "@fastify/swagger": "^8.12.0",
    "@fastify/swagger-ui": "^2.1.0",
    "mediasoup": "^3.12.16",
    "socket.io": "^4.7.4",
    "socket.io-client": "^4.7.4",
    "redis": "^4.6.10",
    "ioredis": "^5.3.2",
    "pg": "^8.11.3",
    "prisma": "^5.7.1",
    "@prisma/client": "^5.7.1",
    "uuid": "^9.0.1",
    "joi": "^17.11.0",
    "pino": "^8.16.2",
    "pino-pretty": "^10.2.3",
    "dotenv": "^16.3.1",
    "jsonwebtoken": "^9.0.2",
    "bcryptjs": "^2.4.3",
    "aws-sdk": "^2.1489.0",
    "@aws-sdk/client-s3": "^3.454.0",
    "@aws-sdk/s3-request-presigner": "^3.454.0",
    "ffmpeg-static": "^5.2.0",
    "fluent-ffmpeg": "^2.1.2",
    "node-cron": "^3.0.3",
    "prometheus-api-metrics": "^3.2.2",
    "opentelemetry-api": "^1.7.0",
    "@opentelemetry/auto-instrumentations-node": "^0.40.0",
    "@opentelemetry/exporter-jaeger": "^1.17.0",
    "@opentelemetry/sdk-node": "^0.45.0"
  },
  "devDependencies": {
    "@types/node": "^20.9.0",
    "@types/uuid": "^9.0.7",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/bcryptjs": "^2.4.6",
    "@types/fluent-ffmpeg": "^2.1.24",
    "@types/jest": "^29.5.8",
    "@types/supertest": "^2.0.16",
    "typescript": "^5.2.2",
    "tsx": "^4.1.4",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "supertest": "^6.3.3",
    "eslint": "^8.53.0",
    "@typescript-eslint/eslint-plugin": "^6.11.0",
    "@typescript-eslint/parser": "^6.11.0",
    "nodemon": "^3.0.1"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}