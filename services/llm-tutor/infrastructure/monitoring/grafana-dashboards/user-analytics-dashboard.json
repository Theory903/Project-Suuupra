{
  "dashboard": {
    "id": null,
    "title": "LLM Tutor - User Learning Analytics",
    "tags": ["llm", "tutor", "user", "analytics", "learning"],
    "style": "dark",
    "timezone": "browser",
    "refresh": "1m",
    "time": {
      "from": "now-6h",
      "to": "now"
    },
    "panels": [
      {
        "id": 1,
        "title": "Active Sessions",
        "type": "stat",
        "targets": [
          {
            "expr": "count(rate(user_session_duration_seconds_count[5m]) > 0)",
            "legendFormat": "Active Sessions"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "thresholds": {
              "steps": [
                {"color": "green", "value": null},
                {"color": "yellow", "value": 50},
                {"color": "red", "value": 200}
              ]
            }
          }
        },
        "gridPos": {"h": 8, "w": 6, "x": 0, "y": 0}
      },
      {
        "id": 2,
        "title": "Average Session Duration",
        "type": "stat",
        "targets": [
          {
            "expr": "histogram_quantile(0.50, rate(user_session_duration_seconds_bucket[1h])) / 60",
            "legendFormat": "Duration (min)"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "min",
            "thresholds": {
              "steps": [
                {"color": "red", "value": null},
                {"color": "yellow", "value": 5},
                {"color": "green", "value": 15}
              ]
            }
          }
        },
        "gridPos": {"h": 8, "w": 6, "x": 6, "y": 0}
      },
      {
        "id": 3,
        "title": "Conversation Completion Rate",
        "type": "stat",
        "targets": [
          {
            "expr": "histogram_quantile(0.50, rate(conversation_completion_rate_bucket[1h])) * 100",
            "legendFormat": "Completion %"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percent",
            "thresholds": {
              "steps": [
                {"color": "red", "value": null},
                {"color": "yellow", "value": 60},
                {"color": "green", "value": 80}
              ]
            }
          }
        },
        "gridPos": {"h": 8, "w": 6, "x": 12, "y": 0}
      },
      {
        "id": 4,
        "title": "User Interactions/Hour",
        "type": "stat",
        "targets": [
          {
            "expr": "rate(user_interactions_total[1h]) * 3600",
            "legendFormat": "Interactions/hr"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "thresholds": {
              "steps": [
                {"color": "green", "value": null},
                {"color": "yellow", "value": 1000},
                {"color": "red", "value": 5000}
              ]
            }
          }
        },
        "gridPos": {"h": 8, "w": 6, "x": 18, "y": 0}
      },
      {
        "id": 5,
        "title": "User Engagement by Subject",
        "type": "piechart",
        "targets": [
          {
            "expr": "sum by (subject_area) (rate(user_interactions_total[1h]))",
            "legendFormat": "{{subject_area}}"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 8}
      },
      {
        "id": 6,
        "title": "User Tier Distribution",
        "type": "piechart",
        "targets": [
          {
            "expr": "sum by (user_tier) (rate(user_interactions_total[1h]))",
            "legendFormat": "{{user_tier}}"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 8}
      },
      {
        "id": 7,
        "title": "Session Duration Distribution",
        "type": "heatmap",
        "targets": [
          {
            "expr": "rate(user_session_duration_seconds_bucket[5m])",
            "format": "heatmap",
            "legendFormat": "{{le}}"
          }
        ],
        "dataFormat": "tsbuckets",
        "yAxis": {
          "unit": "s"
        },
        "gridPos": {"h": 8, "w": 24, "x": 0, "y": 16}
      },
      {
        "id": 8,
        "title": "Learning Progress by Subject Area",
        "type": "graph",
        "targets": [
          {
            "expr": "histogram_quantile(0.50, rate(learning_progress_score_bucket[1h])) by (subject_area)",
            "legendFormat": "{{subject_area}} - Progress Score"
          }
        ],
        "yAxes": [
          {
            "label": "Progress Score",
            "min": 0,
            "max": 1
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 24}
      },
      {
        "id": 9,
        "title": "Completion Rate by Difficulty",
        "type": "graph",
        "targets": [
          {
            "expr": "histogram_quantile(0.50, rate(conversation_completion_rate_bucket[1h])) by (difficulty_level)",
            "legendFormat": "{{difficulty_level}}"
          }
        ],
        "yAxes": [
          {
            "label": "Completion Rate",
            "min": 0,
            "max": 1
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 24}
      },
      {
        "id": 10,
        "title": "User Interaction Patterns",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(user_interactions_total[5m]) by (interaction_type)",
            "legendFormat": "{{interaction_type}}"
          }
        ],
        "yAxes": [
          {
            "label": "Interactions/sec",
            "min": 0
          }
        ],
        "gridPos": {"h": 8, "w": 24, "x": 0, "y": 32}
      },
      {
        "id": 11,
        "title": "Learning Metrics by Assessment Type",
        "type": "table",
        "targets": [
          {
            "expr": "avg_over_time(learning_progress_score[1h]) by (assessment_type, subject_area)",
            "format": "table",
            "instant": true
          }
        ],
        "fieldConfig": {
          "defaults": {
            "custom": {
              "displayMode": "color-background"
            },
            "unit": "percentunit",
            "thresholds": {
              "steps": [
                {"color": "red", "value": null},
                {"color": "yellow", "value": 0.6},
                {"color": "green", "value": 0.8}
              ]
            }
          }
        },
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 40}
      },
      {
        "id": 12,
        "title": "User Retention Funnel",
        "type": "bargauge",
        "targets": [
          {
            "expr": "sum(rate(user_session_duration_seconds_count[1h])) by (session_type)",
            "format": "table",
            "instant": true
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "thresholds": {
              "steps": [
                {"color": "red", "value": null},
                {"color": "yellow", "value": 10},
                {"color": "green", "value": 50}
              ]
            }
          }
        },
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 40}
      },
      {
        "id": 13,
        "title": "Voice Usage Patterns",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(voice_asr_duration_seconds_count[5m])",
            "legendFormat": "ASR Requests"
          },
          {
            "expr": "rate(voice_tts_duration_seconds_count[5m])",
            "legendFormat": "TTS Requests"
          }
        ],
        "yAxes": [
          {
            "label": "Requests/sec",
            "min": 0
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 48}
      },
      {
        "id": 14,
        "title": "Daily Active Users",
        "type": "graph",
        "targets": [
          {
            "expr": "count by (day) (count by (user_id, day) (rate(user_interactions_total[1d])))",
            "legendFormat": "DAU"
          }
        ],
        "yAxes": [
          {
            "label": "Users",
            "min": 0
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 48}
      }
    ],
    "templating": {
      "list": [
        {
          "name": "subject_area",
          "type": "query",
          "query": "label_values(user_interactions_total, subject_area)",
          "current": {
            "text": "All",
            "value": "$__all"
          },
          "includeAll": true,
          "multi": true
        },
        {
          "name": "user_tier",
          "type": "query",
          "query": "label_values(user_interactions_total, user_tier)",
          "current": {
            "text": "All",
            "value": "$__all"
          },
          "includeAll": true,
          "multi": true
        },
        {
          "name": "difficulty_level",
          "type": "query",
          "query": "label_values(conversation_completion_rate, difficulty_level)",
          "current": {
            "text": "All",
            "value": "$__all"
          },
          "includeAll": true,
          "multi": true
        }
      ]
    }
  }
}